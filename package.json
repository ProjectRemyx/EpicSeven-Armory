{
  "name": "epicseven_armory",
  "version": "1.0.0",
  "description": "An armory built for the mobile game Epic Seven. ",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProjectRemyx/EpicSeven-Armory.git"
  },
  "author": "Jing Cheng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ProjectRemyx/EpicSeven-Armory/issues"
  },
  "homepage": "https://github.com/ProjectRemyx/EpicSeven-Armory#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "anchor": "^1.4.0",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.17.1",
    "mdbreact": "^4.16.0",
    "mongoose": "^5.6.0",
    "react-router-dom": "^5.0.1",
    "react-scrollable": "^1.2.1",
    "shards-react": "^1.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
